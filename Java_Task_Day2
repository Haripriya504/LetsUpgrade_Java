public class Emp {
		String name="Haripriya";
		int age=20;
		String city="Bangalore";
		public void display() {	
			System.out.println("The name is "+name);
			System.out.println("The age is "+age); 
			System.out.println("The city is "+city); 
		}
		public static void main(String[] args) {
			Emp e=new Emp();
			Emp e1=new Emp();
			e.display();
			e1.display();
		}
}
